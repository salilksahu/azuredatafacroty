{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "salildatafactory"
		},
		"AzureNSEDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureNSEDB'"
		},
		"nsestorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'nsestorage'"
		},
		"new_sec_file_added_properties_populate_secs_parameters_secfilename": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"new_sec_file_added_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/7d62fa9c-ddf9-43b0-bde2-32606723987c/resourceGroups/StockGroup/providers/Microsoft.Storage/storageAccounts/stockmarketfiles"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "salildatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureNSEDB_connectionString": {
						"value": "[parameters('AzureNSEDB_connectionString')]"
					},
					"nsestorage_connectionString": {
						"value": "[parameters('nsestorage_connectionString')]"
					},
					"new_sec_file_added_properties_populate_secs_parameters_secfilename": {
						"value": "[parameters('new_sec_file_added_properties_populate_secs_parameters_secfilename')]"
					},
					"new_sec_file_added_properties_typeProperties_scope": {
						"value": "[parameters('new_sec_file_added_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}